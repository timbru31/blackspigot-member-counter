{
  "name": "blackspigot-member-counter",
  "version": "0.1.0",
  "description": "A WebDriver backed utility to get the current BlackSpigotMC member count",
  "scripts": {
    "prepublish": "rimraf ./dist",
    "lint": "tslint -p tsconfig.json .",
    "pretest": "tsc -p tsconfig.spec.json",
    "test": "jest",
    "release": "standard-version",
    "start": "ts-node ./src/member-counter.ts"
  },
  "engines": {
    "node": ">=8"
  },
  "author": {
    "name": "timbru"
  },
  "dependencies": {
    "@types/webdriverio": "^4.10.1",
    "ts-node": "^7.0.0",
    "typescript": "^2.8.3",
    "webdriverio": "^4.12.0"
  },
  "devDependencies": {
    "@types/jest": "^23.0.0",
    "@types/node": "^10.1.0",
    "cz-customizable": "^5.2.0",
    "jest": "^22.4.3",
    "rimraf": "^2.6.2",
    "saucelabs": "^1.5.0",
    "standard-version": "^4.3.0",
    "ts-jest": "^23.0.0",
    "tslint": "^5.10.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "license": "MIT",
  "repository": "timbru31/blackspigot-member-counter",
  "bugs": {
    "url": "https://github.com/timbru31/blackspigot-member-counter/issues"
  },
  "keywords": [
    "blackspigot",
    "leak",
    "ban",
    "spigot",
    "bukkit",
    "minecraft",
    "webdriver",
    "webdriverio",
    "selenium"
  ],
  "jest": {
    "transform": {
      "^.+\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(test|spec)\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "reporters": [
      "default",
      [
        "<rootDir>/tmp/sauce-labs-reporter.js",
        {
          "banana": "yes",
          "pineapple": "no"
        }
      ]
    ]
  }
}
