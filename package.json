{
	"name": "blackspigot-member-counter",
	"version": "0.1.0",
	"description": "A WebDriver backed utility to get the current BlackSpigotMC member count",
	"scripts": {
		"postinstall": "husky install",
		"lint": "tslint -p tsconfig.json",
		"pretest": "tsc -p tsconfig.spec.json",
		"test": "jest",
		"prerelease": "rimraf ./dist",
		"release": "standard-version",
		"start": "ts-node ./src/member-counter.ts",
		"prepublishOnly": "pinst --disable",
		"postpublish": "pinst --enable"
	},
	"engines": {
		"node": ">= 12.20",
		"npm": ">= 6.14"
	},
	"author": {
		"name": "timbru"
	},
	"dependencies": {
		"@wdio/sauce-service": "^7.7.3",
		"ts-node": "^10.1.0",
		"typescript": "^4.3.5",
		"webdriverio": "^7.7.3"
	},
	"devDependencies": {
		"@types/jest": "^26.0.24",
		"@types/node": "^12.20.16",
		"husky": "^7.0.1",
		"jest": "^26.6.3",
		"jest-circus": "^27.0.6",
		"lint-staged": "^11.0.1",
		"pinst": "^2.1.6",
		"prettier": "^2.3.2",
		"rimraf": "^3.0.2",
		"saucelabs": "^5.1.0",
		"standard-version": "^9.3.0",
		"ts-jest": "^26.5.6",
		"tslint": "^6.1.3"
	},
	"license": "MIT",
	"repository": "timbru31/blackspigot-member-counter",
	"bugs": {
		"url": "https://github.com/timbru31/blackspigot-member-counter/issues"
	},
	"keywords": [
		"blackspigot",
		"leak",
		"ban",
		"spigot",
		"bukkit",
		"minecraft",
		"webdriver",
		"webdriverio",
		"selenium"
	]
}
