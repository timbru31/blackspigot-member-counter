{
  "name": "blackspigot-member-counter",
  "version": "0.1.0",
  "description": "A WebDriver backed utility to get the current BlackSpigotMC member count",
  "scripts": {
    "prepublish": "rimraf ./dist",
    "lint": "tslint -p tsconfig.json .",
    "test": "jest",
    "release": "standard-version",
    "start": "ts-node ./src/member-counter.ts"
  },
  "engines": {
    "node": ">=8"
  },
  "author": {
    "name": "timbru"
  },
  "dependencies": {
    "@types/webdriverio": "~4.8.0",
    "ts-node": "~3.3.0",
    "typescript": "~2.4.1",
    "webdriverio": "~4.8.0"
  },
  "devDependencies": {
    "@types/jest": "~20.0.2",
    "chromedriver": "~2.31.0",
    "cz-customizable": "~5.0.0",
    "jest": "~20.0.4",
    "rimraf": "~2.6.1",
    "standard-version": "~4.2.0",
    "ts-jest": "~20.0.6",
    "tslint": "~5.6.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "license": "MIT",
  "repository": "timbru31/blackspigot-member-counter",
  "bugs": {
    "url": "https://github.com/timbru31/blackspigot-member-counter/issues"
  },
  "keywords": [
    "blackspigot",
    "leak",
    "ban",
    "spigot",
    "bukkit",
    "minecraft",
    "webdriver",
    "webdriverio",
    "selenium"
  ],
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
