{
	"name": "blackspigot-member-counter",
	"version": "0.2.0",
	"description": "A WebDriver backed utility to get the current BlackSpigotMC member count",
	"scripts": {
		"postinstall": "husky install",
		"lint": "tslint -p tsconfig.json",
		"pretest": "tsc -p tsconfig.spec.json",
		"test": "jest",
		"prerelease": "rimraf ./dist",
		"release": "standard-version",
		"start": "ts-node ./src/member-counter.ts",
		"prepublishOnly": "pinst --disable",
		"postpublish": "pinst --enable"
	},
	"engines": {
		"node": ">= 14.17",
		"npm": ">= 6.14"
	},
	"author": {
		"name": "timbru"
	},
	"dependencies": {
		"@wdio/sauce-service": "^7.19.4",
		"ts-node": "^10.7.0",
		"typescript": "^4.6.3",
		"webdriverio": "^7.19.4"
	},
	"devDependencies": {
		"@types/jest": "^27.4.1",
		"@types/node": "^12.20.47",
		"husky": "^7.0.4",
		"jest": "^27.5.1",
		"jest-circus": "^27.5.1",
		"lint-staged": "^12.3.7",
		"pinst": "^3.0.0",
		"prettier": "^2.6.2",
		"rimraf": "^3.0.2",
		"saucelabs": "^7.1.3",
		"standard-version": "^9.3.2",
		"ts-jest": "^27.1.4",
		"tslint": "^6.1.3"
	},
	"license": "MIT",
	"repository": "timbru31/blackspigot-member-counter",
	"bugs": {
		"url": "https://github.com/timbru31/blackspigot-member-counter/issues"
	},
	"keywords": [
		"blackspigot",
		"leak",
		"ban",
		"spigot",
		"bukkit",
		"minecraft",
		"webdriver",
		"webdriverio",
		"selenium"
	]
}
